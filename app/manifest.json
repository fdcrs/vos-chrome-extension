{
  "name": "RuneScape Voice of Seren",
  "short_name": "VOS",
  "version": "1.2.0",
  "manifest_version": 2,
  "description": "Displays active RuneScape Voice of Seren",
  "icons": {
    "16": "images/icons/logo-16.png",
    "48": "images/icons/logo-48.png",
    "128": "images/icons/logo-128.png"
  },
  "offline_enabled": false,
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icons/Default.png"
    },
    "default_title": "Voice of Seren",
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": false
  },
  "permissions": [
    "notifications"
  ]
}
